                android:text="@string/txtViewProva5Unicamp"
                android:textAlignment="viewStart" />

            <TextView
                android:id="@+id/txtViewGabarito5Unicamp"
                android:layout_width="198dp"
                android:layout_height="50dp"
                android:layout_weight="1"
                android:text="@string/txtViewGabarito5Unicamp"
                android:textAlignment="viewStart" />

            <CheckBox
                android:id="@+id/checkBox5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/txtViewProva6Unicamp"
                android:layout_width="172dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/txtViewProva6Unicamp"
                android:textAlignment="viewStart" />

            <TextView
                android:id="@+id/txtViewGabarito6Unicamp"
                android:layout_width="198dp"
                android:layout_height="50dp"
                android:layout_weight="1"
                android:text="@string/txtViewGabarito6Unicamp"
                android:textAlignment="viewStart" />

            <CheckBox
                android:id="@+id/checkBox6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/txtViewProva7Unicamp"
                android:layout_width="172dp"
