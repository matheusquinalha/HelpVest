<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background_main_activity"
    tools:context=".UnicampBibliotecaActivity"
    >

    <TableRow
        android:id="@+id/tableRow2"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <include
            layout="@layout/toolbar_activity" />
    </TableRow>

    <TextView
        android:id="@+id/textViewBibliotecaFuvest"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="@string/txtViewProvaUnicamp"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tableRow2"
        app:textAllCaps="true" />

    <TableLayout
        android:id="@+id/tableLayout"
        android:layout_width="0dp"
        android:layout_height="673dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="20dp"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewBibliotecaFuvest">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/txtViewProvaUnicamp"
                android:layout_width="105dp"
                android:layout_height="25dp"
                android:layout_weight="1"
                android:text="@string/txtViewProva"
                android:textAlignment="viewStart"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/txtViewGabaritoUnicamp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/txtViewGabarito"
                android:textAlignment="viewStart"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/txtViewFeitoUnicamp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/txtViewFeito"
                android:textAlignment="center"
                android:textStyle="bold" />
        </TableRow>


        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/txtViewProva1Unicamp"
                android:layout_width="172dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/txtViewProva1Unicamp"
                android:textAlignment="viewStart" />

            <TextView
                android:id="@+id/txtViewGabarito1Unicamp"
                android:layout_width="198dp"
                android:layout_height="50dp"
                android:layout_weight="1"
                android:text="@string/txtViewGabarito1Unicamp"
                android:textAlignment="viewStart" />

            <CheckBox
                android:id="@+id/checkBox1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/txtViewProva2Unicamp"
                android:layout_width="172dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/txtViewProva2Unicamp"
                android:textAlignment="viewStart" />

            <TextView
                android:id="@+id/txtViewGabarito2Unicamp"
                android:layout_width="198dp"
                android:layout_height="50dp"
                android:layout_weight="1"
                android:text="@string/txtViewGabarito2Unicamp"
